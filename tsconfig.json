{
	"version": "2.1.4",
	"compilerOptions": {
		"declaration": true,
		"module": "commonjs",
		"outDir": "lib",
		"target": "es5",
		"jsx": "react",
		"experimentalDecorators": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"suppressImplicitAnyIndexErrors": true,
		"strictNullChecks": true
	},
	"files": [
		"src/index.ts",
		"src/bin/cmd.ts",
		"src/ide/main.tsx",
		"src/test/evan.ts",
		"src/test/meta-model.ts"
	]
}